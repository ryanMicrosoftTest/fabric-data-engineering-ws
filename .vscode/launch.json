{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WSL Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "envFile": "${workspaceFolder}/.env",
      "env": {
        "PYTHONUNBUFFERED": "1"
      }
    },
    {
      "name": "WSL Python: Run AML Script",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/aml/consume_employee_uri_folder.py",
      "python": "${workspaceFolder}/.venv/bin/python",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}